{
  "name": "x402-aptos-facilitator",
  "version": "1.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "x402 protocol facilitator SDK for Aptos blockchain - Easy payment verification and middleware",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "ts-node src/facilitator-server.ts",
    "start": "node dist/facilitator-server.js",
    "server": "ts-node src/facilitator-server.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.0.0",
    "express": "^4.18.0"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "x402",
    "aptos",
    "facilitator",
    "middleware",
    "payments",
    "blockchain",
    "sdk",
    "typescript"
  ],
  "author": "x402 Protocol Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/x402-protocol/aptos-facilitator.git"
  },
  "bugs": {
    "url": "https://github.com/x402-protocol/aptos-facilitator/issues"
  },
  "homepage": "https://github.com/x402-protocol/aptos-facilitator#readme",
